<p id="notice"><%= notice %></p>

<h1>Bookings</h1>

<h3>Monday</h3>
<table>
  <thead>
    <tr>
      <th>Room</th>
      <th>Date</th>
      <th>Start Time</th>
      <th>End Time</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.room_id %></td>
        <td><%= select_month(booking.start_time) %></td>
        <td><%= booking.start_time %></td>
        <td><%= booking.finish_time %></td>
        <td><%= booking.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>
